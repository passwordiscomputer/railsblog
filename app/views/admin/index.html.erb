<h1>Admin Page</h1>


<div class="projects">
  <% @projects.each do |project| %>
    <p><%=project.title%></p>
  <% end %>
</div>
<div>
  <%= form_for [@new_project] do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <p><%= f.submit class: "waves-effect waves-light btn-small btn-form"%></p>
  <% end %>
</div>
